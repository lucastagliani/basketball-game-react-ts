{
  "extends": [
    "eslint:recommended",
    "react-app",
    "react-app/jest",
    "airbnb",
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "semi": ["error", "never"],
    "@typescript-eslint/semi": ["error", "never"],
    "unused-imports": 0,
    "no-unused-vars": 0
  },
  "plugins": ["unused-imports"]
}
